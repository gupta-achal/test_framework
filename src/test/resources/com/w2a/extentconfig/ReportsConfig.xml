<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!--
        The theme for the report:
        Options: 'standard', 'dark'
        -->
        <theme>standard</theme>

        <!--
        Document encoding, default is UTF-8
        -->
        <encoding>UTF-8</encoding>

        <!--
        Protocol for loading external scripts and stylesheets
        Default: https
        -->
        <protocol>https</protocol>

        <!--
        Title of the report document (appears in the browser tab)
        -->
        <documentTitle>Automation Reports</documentTitle>

        <!--
        Report name displayed at the top of the report navigation bar
        -->
        <reportName>Automation Report</reportName>

        <!--
        Headline of the report displayed under the report name
        -->
        <reportHeadline>Testing Framework</reportHeadline>

        <!--
        Global date format, defaults to 'yyyy-MM-dd'
        -->
        <dateFormat>yyyy-MM-dd</dateFormat>

        <!--
        Global time format, defaults to 'HH:mm:ss'
        -->
        <timeFormat>HH:mm:ss</timeFormat>

        <!--
        Custom JavaScript for the report
        You can add your own JS/jQuery to enhance the report functionality
        -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                    // Custom JavaScript can be added here
                });
            ]]>
        </scripts>

        <!--
        Custom CSS styles for the report
        You can add CSS to customize the appearance
        -->
        <styles>
            <![CDATA[
                /* Add custom CSS styling for your report here */
            ]]>
        </styles>

        <!--
        Additional configurations or extensions can be added below as needed
        -->
    </configuration>
</extentreports>
