                script {
                    // Use Docker Compose to spin up the services
                    bat &quot;&quot;&quot;
                        docker-compose -f ${DOCKER_COMPOSE_PATH} up --build
                    &quot;&quot;&quot;
                }
            }
        }
    }
    
